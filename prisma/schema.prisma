generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model instructor {
  id       Int       @id @default(autoincrement())
  username String    @unique
  password String
  student  student[]
}

model student {
  id           Int        @id @default(autoincrement())
  name         String
  cohort       String
  instructorid Int
  instructor   instructor @relation(fields: [instructorid], references: [id], onDelete: Cascade, onUpdate: NoAction)
}
